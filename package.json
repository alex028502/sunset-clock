{
  "name": "sunset-clock",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "icons": "make icons",
    "_build": "webpack --devtool source-map",
    "build": "npm run _build && npm run icons",
    "_package": "electron-packager public --platform=linux --arch=x64 --out=dist --tmpdir=./tmp --overwrite",
    "package": "npm run build && npm run _package",
    "_mac-package": "electron-packager public --platform=darwin --arch=x64 --out=dist --tmpdir=./tmp --overwrite --icon=icons.inconset",
    "mac-package": "make icons.icns && npm run build && npm run _mac-package",
    "_start": "webpack-dev-server -d --content-base public --no-inline",
    "start": "npm run icons && npm run _develop",
    "_demo": "node demo/",
    "demo": "npm run build && npm run _demo",
    "_secure": "./scripts/serve.sh",
    "secure": "npm run build && npm run _secure",
    "_electron": "electron public",
    "electron": "npm run build && npm run _electron",
    "unit-test-src": "ls tests/*.js | xargs -n1 babel-node",
    "unit-test-clock-app": "ls tests/clock-app/*.js | xargs -n1 babel-node",
    "unit-test-support": "find . -name \"*.unit.js\" | grep -v node_modules | xargs -n1 babel-node",
    "unit-test-reject": "! node tests/helpers/unhandled-rejection-demo.js",
    "unit-test": "npm run unit-test-src && npm run unit-test-clock-app && npm run unit-test-support && npm run unit-test-reject && echo UNIT TESTS PASS",
    "_electron-e2e-test": "node ./e2e/print-tmp-dir.js | xargs rm -rf && babel-node e2e/electron.js",
    "electron-e2e-test": "npm run package && npm run _electron-e2e-test",
    "_browser-e2e-test": "SELENIUM_PROMISE_MANAGER=0 node_modules/.bin/babel-node e2e/browser.js",
    "browser-e2e-test": "npm run build && npm run _browser-e2e-test",
    "_demo-test": "node demo/test.js",
    "demo-test": "npm run build && npm run _demo-test",
    "lint": "scripts/linted-files.sh | xargs eslint",
    "format": "scripts/linted-files.sh | xargs eslint --fix",
    "_test": "npm run unit-test && npm run _electron-e2e-test && npm run _demo-test && npm run _browser-e2e-test",
    "test": "npm run package && nyc npm run _test && npm run lint"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "coordinate-parser": "^1.0.2",
    "electron": "^1.7.9",
    "electron-packager": "^10.1.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "4.2.0",
    "eslint-config-fbjs": "^1.1.1",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-react": "^7.5.1",
    "formatcoords": "^1.1.3",
    "jest": "^21.2.1",
    "jsdom": "^11.4.0",
    "koa": "^2.4.1",
    "koa-router": "^7.3.0",
    "koa2-static-middleware": "^1.0.3",
    "nyc": "^11.3.0",
    "portfinder": "^1.0.13",
    "prop-types": "^15.5.10",
    "proxyquire": "^1.8.0",
    "raf": "^3.4.0",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "redux": "^3.7.2",
    "selenium-webdriver": "^3.6.0",
    "server-destroy": "^1.0.1",
    "spectron": "^3.7.2",
    "string-replace-webpack-plugin": "^0.1.3",
    "suncalc": "^1.8.0",
    "uuid": "^3.1.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^2.5.1"
  }
}
